use .base.io.IO
use .base.Text
use .base.Int

toUpper : Text -> Text
toUpper input = Text.toUppercase input

addText : Text -> Text -> Text
addText input text = toUpper input ++ text

sumFunc:  Int -> Int -> Int
sumFunc n1 n2 = n1 + n2

transformIntToText: Int -> Text
transformIntToText number = Int.toText(number)

upperHello : '{IO, Exception} ()
-- upperHello _ = printLine (addText "hello, unison!" "!!")
upperHello _ = printLine (transformIntToText(sumFunc +100 +200))